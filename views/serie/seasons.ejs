<%- include('../headeradmin'); -%>


<% if (locals.olddata) { %>

  <div class="container">
<br/>

<a class="waves-effect waves-light btn" href="/admin/seasons/create?id=<%= olddata.id  %>"><i class="material-icons left">add</i>Add Seasons</a>
<br/>
<br/>

<h5>Seasons FOR <%= olddata.name  %></h5>
      <table>
        <thead>
          <tr>
              <th>Action</th>
              <th>Name</th>
          </tr>
        </thead>

        <tbody>

  

   

      
<% if (locals.data) { %>

      <% data.forEach((item)=>{%>

            <tr>
            <td>      
            
             <a  class="waves-effect waves-light btn" href="/admin/seasons/edit?id=<%=item.id %>">Edit </a>
             
          <a  class="waves-effect waves-light btn" href="/admin/seasons/delete?id=<%=item.id %>">Delete </a>


                    <a  class="waves-effect waves-light btn" href="/admin/episodes?id=<%=item.id %>">Episodes </a></td>

            <td><%=item.name %></td>
          </tr>
            
        <%});%>

<% } %>


 
        </tbody>
      </table>




<br/>
<br/>

          
  </div>

<% }%>

